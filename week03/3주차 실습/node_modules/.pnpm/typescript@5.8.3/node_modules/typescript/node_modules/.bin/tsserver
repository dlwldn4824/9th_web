#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/LEE JI WOO/Desktop/대학교 폴더/광운대 2학년 2학기/9th_web/week03/3주차 실습/node_modules/.pnpm/typescript@5.8.3/node_modules/typescript/bin/node_modules:/mnt/c/Users/LEE JI WOO/Desktop/대학교 폴더/광운대 2학년 2학기/9th_web/week03/3주차 실습/node_modules/.pnpm/typescript@5.8.3/node_modules/typescript/node_modules:/mnt/c/Users/LEE JI WOO/Desktop/대학교 폴더/광운대 2학년 2학기/9th_web/week03/3주차 실습/node_modules/.pnpm/typescript@5.8.3/node_modules:/mnt/c/Users/LEE JI WOO/Desktop/대학교 폴더/광운대 2학년 2학기/9th_web/week03/3주차 실습/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/LEE JI WOO/Desktop/대학교 폴더/광운대 2학년 2학기/9th_web/week03/3주차 실습/node_modules/.pnpm/typescript@5.8.3/node_modules/typescript/bin/node_modules:/mnt/c/Users/LEE JI WOO/Desktop/대학교 폴더/광운대 2학년 2학기/9th_web/week03/3주차 실습/node_modules/.pnpm/typescript@5.8.3/node_modules/typescript/node_modules:/mnt/c/Users/LEE JI WOO/Desktop/대학교 폴더/광운대 2학년 2학기/9th_web/week03/3주차 실습/node_modules/.pnpm/typescript@5.8.3/node_modules:/mnt/c/Users/LEE JI WOO/Desktop/대학교 폴더/광운대 2학년 2학기/9th_web/week03/3주차 실습/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/tsserver" "$@"
else
  exec node  "$basedir/../../bin/tsserver" "$@"
fi
